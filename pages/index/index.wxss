/* pages/renthouse/renthouse.wxss */

page{
  font-family: Microsoft YaHei, Arial, Helvetica, sans-serif;
  background: #eceff5;
  height: 100%;
  overflow: hidden;
}
.index{
  
}

.navWrap{
  /* position: -webkit-sticky;
  position: sticky;
  top: -1rpx; */
  margin:0 20rpx;
  height: 90rpx;
  line-height: 90rpx;
  background: #eceff5;
}
.nav{
  height: 90rpx;
  line-height: 90rpx;
  display: inline-block;
  
}
.nav text{
  display: inline-block;
  font-size: 30rpx;
  line-height: 30rpx;
  height: 30rpx;
  padding: 0 30rpx;
  border-right: 1rpx solid #999;
}
.nav.active{
  color: #1296db;
}
.scrollWrap{
  margin-top: 20rpx;
  width: 100%;
  white-space: nowrap;
}
.navItem{
  display: inline-block;
  width: 200rpx;
  height: 100rpx;
  line-height: 100rpx;
  margin-right: 20rpx;
  text-align: center;
  background: #1296db;
  color: #fff;
  border-radius: 10rpx;
}
.navItem:last-child{
  margin-right:0rpx;
}
.productWrap{
  margin:0 20rpx;
  width: 710rpx;
  overflow: hidden;
  border-radius: 20rpx;
  font-size: 0;
}
.productWrap .productItem{
  display: inline-block;
  width: 345rpx;
  padding: 20rpx;
  box-sizing: border-box;
  vertical-align: top;
  background: #fff;
  margin-bottom: 20rpx;
  border-radius: 10rpx;
}
.productWrap .productItem:nth-child(2n){
  margin-left: 20rpx;
}
.productWrap .productItem .productImgWrap,.productWrap .productItem .productImgWrap image{
  width: 300rpx;
  height: 300rpx;
}
.productWrap .productItem .productImgWrap{
  
}
.productName{
  font-size: 30rpx;
  color: #333;
  line-height: 38rpx;
  height: 76rpx;
  margin-top: 20rpx;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  line-clamp: 2;
  -webkit-box-orient: vertical;
}
.productPrice{
  height: 86rpx;
  display: flex;
  align-items: center; /*定义body的元素垂直居中*/
}
.productPrice .productPriceWrap{
  flex: 1;
}
.productPrice .productPriceWrap text{
  display: block;
}
.productPrice .productPriceWrap .salePrice{
  color: red;
  font-size: 40rpx;
}
.productPrice .productPriceWrap .originalPrice{
  color: #aaa;
  font-size: 30rpx;
  text-decoration-line: line-through;
}
.number{
  height: 50rpx;
  line-height: 50rpx;
  color: #1296db;
  font-size: 32rpx;
  vertical-align: top;
  padding: 0 14rpx;
  display: inline-block;
}
.productAdd {
  display: inline-block;
  width: 50rpx;
  height: 50rpx;
  background: #1296db;
  color: #fff;
  transition: color .25s;
  position: relative;
  border-radius: 50%;
}
.productAdd::before{
  vertical-align: top;
  content: '';
  position: absolute;
  left: 50%;
  top: 50%;
  width: 30rpx;
  margin-left: -15rpx;
  margin-top: -2rpx;
  border-top: 4rpx solid #fff;
}
.productAdd::after {
 content: '';
 position: absolute;
 left: 50%;
 top: 50%;
 height: 30rpx;
 margin-left: -2rpx;
 margin-top: -15rpx;
 border-left: 4rpx solid #fff;
}
.increase {
  vertical-align: top;
  border:1rpx solid #1296db;
  display: inline-block;
  width: 50rpx;
  height: 50rpx;
  color: #1296db;
  transition: color .25s;
  position: relative;
  border-radius: 50%;
}
.increase::before{
  vertical-align: top;
  content: '';
  position: absolute;
  left: 50%;
  top: 50%;
  width: 30rpx;
  margin-left: -15rpx;
  margin-top: -2rpx;
  border-top: 4rpx solid #1296db;
}
.cartBox{
  height: 140rpx;
}
.cartWrap{
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  height: 90rpx;
  display: flex;
  background: #fff;
  z-index: 3;
  border-bottom: 1rpx solid #efefef;
  border-top: 1rpx solid #efefef;
  font-size: 0;
  box-sizing: border-box;
}
.cartWrap.hide{
  opacity: 0;
}
.cartWrap .cart{
  background: #ccc;
  height: 90rpx;
  width: 90rpx;
  border-radius: 50%;
  margin: -20rpx 0 0 20rpx;
}
.cartWrap .cart.active{
  background: #1296db;
}
.cartWrap .cart image{
  height: 70rpx;
  width: 70rpx;
  padding: 10rpx;
}
.buyNum{
  position: absolute;
  border-radius: 50%;
  background: red;
  width: 32rpx;
  height: 32rpx;
  line-height: 32rpx;
  font-size: 24rpx;
  display: inline-block;
  color: #fff;
  margin-left: -26rpx;
  text-align: center;
  box-sizing: border-box;
  vertical-align: top;
}
.buyNum{
  width: 32rpx;
  height: 32rpx;
  line-height: 32rpx;
}
.maxnum{
  width: 42rpx;
  height: 42rpx;
  padding: 9rpx;
  margin-left: -26rpx;
}
.cartWrap .price{
  color: red;
  font-size: 40rpx;
  height: 90rpx;
  line-height: 90rpx;
  flex: 1;
}
.cartWrap .confrim{
  height: 90rpx;
  line-height: 90rpx;
  background: #1296db;
  width: 200rpx;
  text-align: center;
  font-size: 30rpx;
  color: #fff;
}
.cartWrap .confrim.gray{
  background: #ccc;
}
.dialogWrap{
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.3);
  z-index: 1;
  overflow: hide;
}
.gouwuche{
  position: fixed;
  bottom: 90rpx;
  /* bottom: 0; */
  left: 0;
  right: 0;
  z-index: 1;
  
}
.itemWrap{
  max-height: 520rpx;
  background: #fff;
  overflow: auto;
}
.deleteAll{
  background: #fff;
  text-align: right;
  font-size: 26rpx;
  color: #999;
  height: 60rpx;
  line-height: 60rpx;
  box-sizing: border-box;
}
.deleteAll .deleteBtn{
  display: inline-block;
  padding: 0 30rpx 0 10rpx;
}
.deleteAll .deleteBtn image{
  width: 40rpx;
  height: 40rpx;
  vertical-align: middle;
  margin-top: -2rpx;
}
.itemWrap{
  max-height: 560rpx;
  overflow: auto;
}
 .gouwuche .gouwucheItem{
  display: flex;
  margin: 0 30rpx;
  padding: 20rpx 0;
  border-bottom: 1rpx solid #eee;
}
 .gouwuche .gouwucheItem image{
  width: 100rpx;
  height: 100rpx;
  margin-right: 20rpx;
}
 .gouwuche .gouwucheItem .itemInfo{
  flex: 1;
}
 .gouwuche .gouwucheItem .itemName{
  font-size: 30rpx;
}
 .gouwuche .gouwucheItem  .itemPrice{
  display: flex;
}
 .gouwuche .gouwucheItem  .itemPrice .salePrice{
  color: red;
  font-size: 26rpx;
  flex: 1;
}
 .gouwuche .gouwucheItem  .itemPrice .salePrice text{
  color: #999;
  text-decoration-line: line-through;
}
 .gouwuche .gouwucheItem  .itemPrice  .itemNumWrap{
  font-size: 36rpx;
}
.itemNumDel,.itemNumAdd{
  width: 42rpx;
  height: 42rpx;
  display: inline-block;
  border-radius: 50%;
  text-align: center;
}
.itemNum{
  display: inline-block;
  color: #333;
  padding: 0 12rpx;
  vertical-align: top;
}
.itemNumAdd{
  display: inline-block;
  background: #1296db;
  color:#fff;
}
.itemNumDel{
  border: 1rpx solid #eee;
}
.topWrap{
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  z-index: 1;
  background: #eceff5;
}
.searchWrap{
  background: #fff;
  height: 70rpx;
  line-height: 70rpx;
  margin: 20rpx 20rpx 0;
  border-radius: 20rpx;
  text-align: center;
}
.topBox{
  height: 206rpx;
}
.searchWrap image{
  width: 50rpx;
  height: 50rpx;
  margin-top: 5rpx;
  vertical-align: top;
}
.searchWrap .searchText{
  color: #707070;
  font-size: 30rpx;
  padding-left: 12rpx;
}