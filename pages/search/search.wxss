.search{
  margin: 20rpx;
}
.searchWrap{
  height: 70rpx;
  line-height: 70rpx;
  display: flex;
  align-items: center; /*定义body的元素垂直居中*/
  justify-content: center; /*定义body的里的元素水平居中*/
  position: relative;
}
input{
  height: 60rpx;
  background: #fafafa;
  border-radius: 60rpx;
  /* width: 590rpx; */
  flex: 1;
  padding: 0 20rpx;
}
.searchWrap cover-image{
  /* position: absolute; */
  /* margin-left: -50rpx; */
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  z-index: 2;
  right: 104rpx;
  margin-right: 10rpx;
  padding: 20rpx;
}
.searchBtn{
  display: inline-block;
  width: 100rpx;
  text-align: center;
  font-size: 26rpx;
  color: #333;
}
.searchBtn image{
  width: 44rpx;
  height: 44rpx;
}
.searchBtn text{
  position: absolute;
  vertical-align: top;
  background: red;
  color: #fff;
  height: 26rpx;
  /* min-width: 26rpx; */
  line-height: 26rpx;
  font-size: 26rpx;
  padding: 0 6rpx;
  border-radius: 50%;
}
.title{
  margin-top: 40rpx;
  line-height: 80rpx;
  font-size: 30rpx;
}
.title .clear{
  /* display: inline-block; */
  line-height: 50rpx;
  margin-top: 15rpx;
  float: right;
  color: #999;
  font-size: 26rpx;
}
.title .clear image{
  vertical-align: middle;
  width: 40rpx;
  height: 40rpx;
}
.itemWrap{
  
}
.item{
  height: 50rpx;
  line-height: 50rpx;
  background: #eee;
  padding: 0 20rpx;
  display: inline-block;
  margin:20rpx 20rpx 0 0;
  border-radius: 50rpx;
  color: #333;
  font-size: 26rpx;
}

.productWrap{
  width: 710rpx;
  overflow: hidden;
  background: #fff;
  border-radius: 20rpx;
  font-size: 0;
}
.productWrap .productItem{
  display: inline-block;
  width: 345rpx;
  box-sizing: border-box;
  vertical-align: top;
  padding: 20rpx;
  border: 1rpx solid #ccc;
  margin-top: 20rpx;
  border-radius: 20rpx;
}
.productWrap .productItem:nth-child(2n){
  margin-left: 20rpx;
}
.productWrap .productItem .productImgWrap,.productWrap .productItem .productImgWrap image{
  width: 300rpx;
  height: 300rpx;
}
.productWrap .productItem .productImgWrap{
  /* padding: 30rpx 0; */
}
.productName{
  font-size: 30rpx;
  color: #333;
  line-height: 36rpx;
  height: 76rpx;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  line-clamp: 2;
  -webkit-box-orient: vertical;
}
.productPrice{
  height: 86rpx;
  display: flex;
  align-items: center; /*定义body的元素垂直居中*/
}
.productPrice .productPriceWrap{
  flex: 1;
}
.productPrice .productPriceWrap text{
  display: block;
}
.productPrice .productPriceWrap .salePrice{
  color: red;
  font-size: 40rpx;
}
.productPrice .productPriceWrap .originalPrice{
  color: #aaa;
  font-size: 30rpx;
  text-decoration-line: line-through;
}
.productPrice .productNumWrap{

}
.number{
  height: 50rpx;
  color: #1296db;
  font-size: 32rpx;
  vertical-align: top;
  padding: 0 14rpx;
  display: inline-block;
}
.productAdd {
  display: inline-block;
  width: 50rpx;
  height: 50rpx;
  background: #1296db;
  color: #fff;
  transition: color .25s;
  position: relative;
  border-radius: 50%;
}
.productAdd::before{
  vertical-align: top;
  content: '';
  position: absolute;
  left: 50%;
  top: 50%;
  width: 30rpx;
  margin-left: -15rpx;
  margin-top: -2rpx;
  border-top: 4rpx solid #fff;
}
.productAdd::after {
 content: '';
 position: absolute;
 left: 50%;
 top: 50%;
 height: 30rpx;
 margin-left: -2rpx;
 margin-top: -15rpx;
 border-left: 4rpx solid #fff;
}
.increase {
  vertical-align: top;
  border:1rpx solid #1296db;
  display: inline-block;
  width: 50rpx;
  height: 50rpx;
  color: #1296db;
  transition: color .25s;
  position: relative;
  border-radius: 50%;
}
.increase::before{
  vertical-align: top;
  content: '';
  position: absolute;
  left: 50%;
  top: 50%;
  width: 30rpx;
  margin-left: -15rpx;
  margin-top: -2rpx;
  border-top: 4rpx solid #1296db;
}
.cartBox{
  height: 140rpx;
}
.cartWrap{
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  height: 90rpx;
  display: flex;
  background: #fff;
  z-index: 2;
  border-bottom: 1rpx solid #efefef;
  font-size: 0;
  border-top: 1rpx solid #efefef;
  box-sizing: border-box;
}
.cartWrap .cart{
  background: #ccc;
  height: 90rpx;
  width: 90rpx;
  border-radius: 50%;
  margin: -20rpx 0 0 20rpx;
}
.cartWrap .cart.active{
  background: #1296db;
}
.cartWrap .cart image{
  height: 70rpx;
  width: 70rpx;
  padding: 10rpx;
}
.buyNum{
  position: absolute;
  border-radius: 50%;
  background: red;
  width: 32rpx;
  height: 32rpx;
  line-height: 32rpx;
  font-size: 24rpx;
  display: inline-block;
  color: #fff;
  margin-left: -26rpx;
  text-align: center;
  box-sizing: border-box;
  vertical-align: top;
}
.buyNum{
  width: 32rpx;
  height: 32rpx;
  line-height: 32rpx;
}
.maxnum{
  width: 42rpx;
  height: 42rpx;
  padding: 9rpx;
  margin-left: -26rpx;
}
.cartWrap .price{
  color: red;
  font-size: 40rpx;
  height: 90rpx;
  line-height: 90rpx;
  flex: 1;
}
.cartWrap .confrim{
  height: 90rpx;
  line-height: 90rpx;
  background: #1296db;
  width: 200rpx;
  text-align: center;
  font-size: 30rpx;
  color: #fff;
}
.cartWrap .confrim.gray{
  background: #ccc;
}
.dialogWrap{
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 120rpx;
  background: rgba(0, 0, 0, 0.3);
  z-index: 1;
}
.gouwuche{
  position: fixed;
  bottom: 120rpx;
  /* bottom: 0; */
  left: 0;
  right: 0;
  z-index: 1;
  max-height: 620rpx;
  background: #fff;
  overflow: auto;
}
.deleteAll{
  text-align: right;
  font-size: 26rpx;
  color: #999;
  padding: 10rpx;
}
.deleteAll .deleteBtn{
  display: inline-block;
}
.itemWrap{
  max-height: 520rpx;
  background: #fff;
  overflow: auto;
}
 .gouwuche .gouwucheItem{
  display: flex;
  margin: 0 30rpx;
  padding: 20rpx 0;
  border-bottom: 1rpx solid #eee;
}
 .gouwuche .gouwucheItem image{
  width: 100rpx;
  height: 100rpx;
  margin-right: 20rpx;
}
 .gouwuche .gouwucheItem .itemInfo{
  flex: 1;
}
 .gouwuche .gouwucheItem .itemName{
  font-size: 30rpx;
}
 .gouwuche .gouwucheItem  .itemPrice{
  display: flex;
}
 .gouwuche .gouwucheItem  .itemPrice .salePrice{
  color: red;
  font-size: 26rpx;
  flex: 1;
}
 .gouwuche .gouwucheItem  .itemPrice .salePrice text{
  color: #999;
  text-decoration-line: line-through;
}
 .gouwuche .gouwucheItem  .itemPrice  .itemNumWrap{
  font-size: 36rpx;
}
.itemNumDel,.itemNumAdd{
  width: 42rpx;
  height: 42rpx;
  display: inline-block;
  border-radius: 50%;
  text-align: center;
}
.itemNum{
  display: inline-block;
  color: #333;
  padding: 0 12rpx;
  vertical-align: top;
}
.itemNumAdd{
  display: inline-block;
  background: #1296db;
  color:#fff;
}
.itemNumDel{
  border: 1rpx solid #eee;
}
.nodata{
  height: 60vh;
}